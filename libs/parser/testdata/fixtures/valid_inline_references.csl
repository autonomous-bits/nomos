source:
	alias: 'network'
	type: 'folder'
	path: './config'

source:
	alias: 'security'
	type: 'folder'
	path: './security'

infrastructure:
	vpc_cidr: @network:config.vpc.cidr
	region: 'us-west-2'
	subnet: @network:config.subnets.public.az1
	security_group: @security:config.rules.ingress.http

application:
	name: 'my-app'
	port: '8080'
	database_host: @network:config.rds.endpoint
